unused rule: '+'
unused rule: '*'

rule expr
    reference <EOF>
        prefixed-by
            reference <add>

rule EOF
    not
        any-character
        prefixed-by
            reference <_>

rule add
    zero-or-more
        reference <mul>
            prefixed-by
                match "+"
                    prefixed-by
                        reference <_>
        prefixed-by
            reference <mul>

rule mul
    zero-or-more
        reference <prim>
            prefixed-by
                match "*"
                    prefixed-by
                        reference <_>
        prefixed-by
            reference <prim>

rule prim
    choice
        one-or-more
            match "a" .. "z"
            prefixed-by
                reference <_>
        one-or-more
            match "0" .. "9"
            prefixed-by
                reference <_>

rule _
    zero-or-more
        reference <space>

rule space
    match " "

rule '+'
    match "+"
        prefixed-by
            reference <_>

rule '*'
    match "*"
        prefixed-by
            reference <_>
